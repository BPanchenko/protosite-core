{
  "name": "@bpanchenko/core",
  "version": "1.5.18",
  "description": "Web Application Core. It —Åontains a library of Web Components.",
  "homepage": "http://protosite.rocks",
  "type": "module",
  "main": "./index.js",
  "types": "./@types/index.d.ts",
  "files": [
    "@types/*",
    "assets/*.js"
  ],
  "exports": {
    ".": "./index.js",
    "./avatar": "./assets/avatar-component.mjs",
    "./field": "./assets/field-component.mjs"
  },
  "imports": {
    "#types": "@types/index.d.ts",
    "#assets/*": "assets/*",
    "#uikit/*": "@bpanchenko/uikit/*"
  },
  "scripts": {
    "build": "node ./.kernel/builder",
    "lint": "eslint . --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --config=.config/jest-unit.cjs",
    "test:e2e": "jest --config=.config/jest-e2e.cjs",
    "server": "node ./.kernel/server.cjs",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@bpanchenko/uikit": "4.28.97",
    "@eslint/js": "9.15.0",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-terser": "0.4.4",
    "@stylistic/eslint-plugin-js": "2.10.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "babel-cli": "6.26.0",
    "babel-jest": "29.7.0",
    "babel-plugin-transform-import-meta": "2.2.1",
    "construct-style-sheets-polyfill": "3.1.0",
    "date-format": "4.0.14",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.9.0",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-require-sort": "1.3.0",
    "eslint-plugin-testing-library": "6.4.0",
    "express": "4.21.1",
    "globals": "15.12.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-image-snapshot": "6.4.0",
    "jest-puppeteer": "10.1.4",
    "lodash": "4.17.21",
    "nocache": "4.0.0",
    "node-color-log": "12.0.1",
    "portscanner": "2.2.0",
    "pptr-testing-library": "0.8.0",
    "prettier": "3.3.3",
    "puppeteer": "23.8.0",
    "puppeteer-core": "23.8.0",
    "rollup": "4.27.2",
    "setimmediate": "1.0.5"
  },
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  },
  "repository": "https://github.com/BPanchenko/protosite-core",
  "license": "LGPL-3.0-only",
  "keywords": [
    "web-components",
    "custom-elements",
    "unit-testing",
    "end-to-end-testing",
    "shadow-dom"
  ]
}
