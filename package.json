    "jest": "29.7.0",
    "jest-environment-happy-dom": "0.0.44",
    "jest-environment-jsdom": "29.7.0",
    "jest-image-snapshot": "6.4.0",
    "jest-puppeteer": "11.0.0",
    "jest-transform-pug": "1.0.0",
    "lodash": "4.17.21",
    "nocache": "4.0.0",
    "node-color-log": "12.0.1",
    "npm-run-all": "4.1.5",
    "portscanner": "2.2.0",
    "pptr-testing-library": "0.8.0",
    "prettier": "3.5.2",
    "pug": "3.0.3",
    "puppeteer": "24.3.0",
    "puppeteer-core": "24.3.0",
