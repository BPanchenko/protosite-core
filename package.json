{
  "name": "@bpanchenko/core",
  "version": "1.15.57",
  "type": "module",
  "main": "./bundle/index.js",
  "types": "./bundle/index.d.ts",
  "files": [
    "bundle/*"
  ],
  "exports": {
    ".": "./bundle/index.js",
    "./esm/*": "./bundle/*",
    "./arrow-element": "./bundle/element.arrow.mjs",
    "./avatar-component": "./bundle/component.avatar.mjs",
    "./listbox-element": "./bundle/element.listbox.mjs",
    "./option-element": "./bundle/element.option.mjs",
    "./select-component": "./bundle/component.select.mjs",
    "./src/*": "./source/*",
    "./component/*": "./source/component/*",
    "./element/*": "./source/element/*",
    "./library/*": "./source/library/*"
  },
  "imports": {
    "#element/*": "./source/element/*",
    "#library/*": "./source/library/*",
    "#uikit/*": "@bpanchenko/uikit/*"
  },
  "scripts": {
    "build": "node ./.kernel/builder.js",
    "debug": "run-p server watch",
    "deploy": "node ./.kernel/deployer.js",
    "lint": "eslint . --fix",
    "postpublishOnly": "npm run deploy",
    "prebuild": "rimraf \"bundle/*.{mjs,mts}\" --glob",
    "server": "node ./.kernel/server.cjs",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "jest --config=.config/jest-e2e.cjs",
    "test:unit": "jest --config=.config/jest-unit.cjs --watch",
    "watch": "npx webpack watch --color --progress --stats -c ./.config/webpack.development.ts"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
