    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest": "27.2.0",
    "jest": "29.3.1",
    "lodash": "4.17.21",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "puppeteer": "19.4.1",
    "rimraf": "3.0.2",
    "typescript": "4.9.4"
  },
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/protosite-core",
