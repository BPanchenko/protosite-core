{
	"targets": "defaults",
	"assumptions": {
		"enumerableModuleMeta": true
	},
	"sourceType": "module",
	"plugins": [
		"babel-plugin-transform-import-meta",
		[
			"@babel/plugin-transform-modules-commonjs",
			{
				"allowTopLevelThis": true,
				"importInterop": "babel"
			}
		]
	],
	"presets": ["@babel/preset-env"]
}