{
  "name": "@bpanchenko/core",
  "private": true,
  "version": "1.15.66",
  "description": "The library `Protosite Core` is collection of interactive web components and graphical interface elements.",
  "homepage": "https://protosite.xyz",
  "type": "module",
  "main": "./.bundle/index.js",
  "types": "./.bundle/index.d.ts",
  "files": [
    "./.bundle/*"
  ],
  "exports": {
    "./arrow-element": "./.bundle/element.arrow.js",
    "./avatar-component": "./.bundle/component.avatar.js",
    "./listbox-element": "./.bundle/element.listbox.js",
    "./option-element": "./.bundle/element.option.js",
    "./select-component": "./.bundle/component.select.js"
  },
  "imports": {
    "#stylesheet/*": "./.bundle/stylesheet.*",
    "#element/*": "./source/element/*",
    "#library/*": "./source/library/*",
    "#uikit/*": "@bpanchenko/uikit/*"
  },
  "scripts": {
    "build:esmodules": "node ./.kernel/build.esmodules.js",
    "build:package": "node ./.kernel/build.package.js",
    "build:stylesheets": "node ./.kernel/build.stylesheets.js",
    "build": "run-s build:stylesheets build:esmodules build:package",
    "deploy": "node ./.kernel/deployer.js",
    "lint": "eslint . --fix",
    "postpublishOnly": "npm run deploy",
    "prebuild": "rimraf \"./.bundle/*.{js,ts}\" --glob",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "jest --config=.config/jest-e2e.cjs",
    "test:unit": "jest --config=.config/jest-unit.cjs"
  },
  "devDependencies": {
    "@bpanchenko/uikit": "file:../protosite-uikit",
    "@csstools/custom-units": "0.1.1",
    "@happy-dom/jest-environment": "18.0.1",
    "@stylistic/eslint-plugin-js": "4.4.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@types/lodash": "4.17.17",
    "@types/postcss-safe-parser": "5.0.4",
    "@types/pug": "2.0.10",
    "construct-style-sheets-polyfill": "3.1.0",
    "cssnano": "7.0.7",
    "cssnano-preset-default": "7.0.7",
    "date-format": "4.0.14",
    "element-internals-polyfill": "3.0.2",
    "esbuild": "0.25.5",
    "esbuild-plugin-d.ts": "1.3.1",
    "express": "5.1.0",
    "glob": "11.0.2",
    "globals": "16.2.0",
    "jest": "30.0.0",
    "jest-environment-jsdom": "30.0.0",
    "jest-image-snapshot": "6.5.1",
    "jest-puppeteer": "11.0.0",
    "jest-transform-pug": "1.0.1",
    "lodash": "4.17.21",
    "nocache": "4.0.0",
    "node-color-log": "12.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.4",
    "postcss-custom-media": "11.0.6",
    "postcss-custom-selectors": "8.0.5",
    "postcss-extend-rule": "4.0.0",
    "postcss-import": "16.1.0",
    "postcss-preset-env": "10.2.3",
    "postcss-prune-var": "1.1.2",
    "postcss-safe-parser": "7.0.1",
    "pptr-testing-library": "0.8.0",
    "pug": "3.0.3",
    "puppeteer": "24.10.0",
    "puppeteer-core": "24.10.0",
    "rimraf": "6.0.1",
    "setimmediate": "1.0.5",
    "typescript": "5.8.3"
  },
  "keywords": [
    "WEB Components",
    "Custom Elements",
    "Unit testing",
    "End-to-End testing"
  ],
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  },
  "license": "LGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BPanchenko/protosite-core.git"
  }
}
