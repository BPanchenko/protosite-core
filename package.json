{
  "name": "@bpanchenko/core",
  "version": "1.7.27",
  "description": "Web Application Core. It —Åontains a library of Web Components.",
  "homepage": "http://protosite.rocks",
  "type": "module",
  "main": "./index.js",
  "types": "./@types/index.d.ts",
  "files": [
    "index.js",
    "@types/*",
    "assets/*.html",
    "assets/*.js",
    "assets/*.mjs",
    "src/*"
  ],
  "exports": {
    "./component/*.js": "./src/components/*.js",
    "./lib/*.js": "./src/lib/*.js",
    "./select-field": "./assets/select-field.mjs",
    "./avatar": "./assets/avatar-component.mjs",
    "./field": "./assets/field-component.mjs",
    "./*.js": "./src/*.js",
    ".": "./index.js"
  },
  "imports": {
    "#types": "@types/index.d.ts",
    "#assets/*": "assets/*",
    "#uikit/*": "@bpanchenko/uikit/*"
  },
  "scripts": {
    "build": "node ./.kernel/builder",
    "debug": "run-p server watch",
    "deploy": "node ./.kernel/deployer.js",
    "lint": "eslint . --fix",
    "postpublish": "npm run deploy",
    "prebuild": "npm run rimraf",
    "prepublish": "npm run build",
    "rimraf": "rimraf \"assets/*.{js,mjs,map}\" --glob",
    "server": "node ./.kernel/server.cjs",
    "test:e2e": "jest --config=.config/jest-e2e.cjs",
    "test:unit": "jest --config=.config/jest-unit.cjs --watch",
    "test": "npm run test:unit && npm run test:e2e",
    "watch": "node ./.kernel/watcher.js"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@bpanchenko/uikit": "4.31.104",
    "@eslint/js": "9.16.0",
    "@github/template-parts": "0.5.4",
    "@html-eslint/eslint-plugin": "0.29.0",
    "@html-eslint/parser": "0.29.0",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-terser": "0.4.4",
    "@stylistic/eslint-plugin-js": "2.11.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "babel-cli": "6.26.0",
    "babel-jest": "29.7.0",
    "babel-plugin-transform-import-meta": "2.2.1",
    "basic-ftp": "5.0.5",
    "construct-style-sheets-polyfill": "3.1.0",
    "date-format": "4.0.14",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.9.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-require-sort": "1.3.0",
    "eslint-plugin-testing-library": "7.0.0",
    "express": "4.21.1",
    "glob": "11.0.0",
    "globals": "15.13.0",
    "jest": "29.7.0",
    "jest-environment-happy-dom": "0.0.44",
    "jest-environment-jsdom": "29.7.0",
    "jest-image-snapshot": "6.4.0",
    "jest-puppeteer": "10.1.4",
    "lodash": "4.17.21",
    "nocache": "4.0.0",
    "node-color-log": "12.0.1",
    "npm-run-all": "4.1.5",
    "portscanner": "2.2.0",
    "pptr-testing-library": "0.8.0",
    "prettier": "3.4.1",
    "pug": "3.0.3",
    "puppeteer": "23.9.0",
    "puppeteer-core": "23.9.0",
    "rimraf": "6.0.1",
    "rollup": "4.28.0",
    "setimmediate": "1.0.5",
    "typescript": "5.7.2",
    "watcher": "2.3.1"
  },
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  },
  "repository": "https://github.com/BPanchenko/protosite-core",
  "license": "LGPL-3.0-only",
  "keywords": [
    "web-components",
    "custom-elements",
    "unit-testing",
    "end-to-end-testing",
    "shadow-dom"
  ]
}
