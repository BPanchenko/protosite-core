{
  "name": "@bpanchenko/core",
  "version": "1.14.53",
  "description": "Web Application Core. It сontains a library of Web Components.",
  "keywords": [
    "WEB Components",
    "Custom Elements",
    "Unit testing",
    "End-to-End testing"
  ],
  "homepage": "http://protosite.rocks",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "assets/*.mjs",
    "assets/*.mts",
    "index.d.ts",
    "index.js"
  ],
  "exports": {
    ".": "./index.js",
    "./*": "./source/*"
  },
  "imports": {
    "#element/*": "./source/element/*",
    "#library/*": "./source/library/*",
    "#types": "./index.d.ts",
    "#type/*": "./@types/*"
  },
  "scripts": {
    "build": "npx webpack build --config ./.config/webpack.config.js --mode production",
    "debug": "run-p server watch",
    "deploy": "node ./.kernel/deployer.js",
    "lint": "eslint . --fix",
    "postbuild": "npm run deploy",
    "prebuild": "npm run rimraf",
    "prepublishOnly": "npm run build",
    "rimraf": "rimraf \"assets/*.{mjs,mts,map}\" --glob",
    "server": "node ./.kernel/server.cjs",
    "test:e2e": "jest --config=.config/jest-e2e.cjs",
    "test:unit": "jest --config=.config/jest-unit.cjs --watch",
    "test": "npm run test:unit && npm run test:e2e",
    "typing": "npx -p typescript tsc --project ./.config/tsconfig.declaration.json",
    "watch": "npx webpack watch --config ./.config/webpack.config.js --color --analyze --progress --stats"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@babel/eslint-parser": "7.26.8",
    "@babel/parser": "7.26.9",
    "@babel/plugin-transform-typescript": "7.26.8",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-typescript": "7.26.0",
    "@babel/types": "7.26.9",
    "@bpanchenko/uikit": "4.37.118",
    "@eslint/js": "9.21.0",
    "@html-eslint/eslint-plugin": "0.35.0",
    "@html-eslint/parser": "0.35.0",
    "@stylistic/eslint-plugin-js": "4.0.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "@typescript/lib-dom": "npm:@types/web",
    "@webdiscus/pug-loader": "2.11.1",
    "babel-cli": "6.26.0",
    "babel-jest": "29.7.0",
    "babel-loader": "9.2.1",
    "babel-plugin-transform-import-meta": "2.3.2",
    "basic-ftp": "5.0.5",
    "construct-style-sheets-polyfill": "3.1.0",
    "core-js": "3",
    "css-loader": "7.1.2",
    "date-format": "4.0.14",
    "element-internals-polyfill": "3.0.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-require-sort": "1.3.0",
    "eslint-plugin-testing-library": "7.1.1",
    "express": "4.21.2",
    "glob": "11.0.1",
