{
  "name": "@bpanchenko/core",
  "version": "1.13.35",
  "description": "Web Application Core. It —Åontains a library of Web Components.",
  "homepage": "http://protosite.rocks",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "assets/*.mjs",
    "assets/*.mts",
    "source/*",
    "index.js",
    "index.d.ts"
  ],
  "exports": {
    ".": "./index.js",
    "./*": "./source/*"
  },
  "imports": {
    "#element/*": "./source/element/*",
    "#library/*": "./source/library/*",
    "#types": "./source/types",
    "#types/*": "./source/types/*"
  },
  "scripts": {
    "build": "node ./.kernel/builder",
    "debug": "run-p server watch",
    "postbuild": "npx -p typescript tsc --project tsconfig.assets.json",
    "deploy": "node ./.kernel/deployer.js",
    "lint": "eslint . --fix",
    "postpublish": "npm run deploy",
    "prebuild": "npm run rimraf",
    "prepublish": "npm run build",
    "rimraf": "rimraf \"assets/*.{mjs,mts,map}\" --glob",
    "server": "node ./.kernel/server.cjs",
    "test:e2e": "jest --config=.config/jest-e2e.cjs",
    "test:unit": "jest --config=.config/jest-unit.cjs --watch",
    "test": "npm run test:unit && npm run test:e2e",
    "watch": "node ./.kernel/watcher.js"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@bpanchenko/uikit": "4.32.104",
    "@eslint/js": "9.17.0",
    "@html-eslint/eslint-plugin": "0.32.0",
    "@html-eslint/parser": "0.32.0",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-typescript": "12.1.2",
    "@stylistic/eslint-plugin-js": "2.12.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "@typescript/lib-dom": "npm:@types/web",
    "babel-cli": "6.26.0",
    "babel-jest": "29.7.0",
    "babel-plugin-transform-import-meta": "2.3.2",
    "basic-ftp": "5.0.5",
    "construct-style-sheets-polyfill": "3.1.0",
    "date-format": "4.0.14",
    "element-internals-polyfill": "1.3.12",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.10.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-require-sort": "1.3.0",
    "eslint-plugin-testing-library": "7.1.1",
    "express": "4.21.2",
    "glob": "11.0.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "jest-environment-happy-dom": "0.0.44",
    "jest-environment-jsdom": "29.7.0",
    "jest-image-snapshot": "6.4.0",
    "jest-puppeteer": "11.0.0",
    "jest-transform-pug": "1.0.0",
    "lodash": "4.17.21",
    "nocache": "4.0.0",
    "node-color-log": "12.0.1",
    "npm-run-all": "4.1.5",
    "portscanner": "2.2.0",
    "pptr-testing-library": "0.8.0",
    "prettier": "3.4.2",
    "pug": "3.0.3",
    "puppeteer": "23.11.1",
    "puppeteer-core": "23.11.1",
    "rimraf": "6.0.1",
    "rollup": "4.30.1",
    "rollup-plugin-pug": "1.1.1",
    "setimmediate": "1.0.5",
    "typescript": "5.7.2",
    "watcher": "2.3.1"
  },
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  },
