{
  "name": "protosite-core",
  "version": "1.3.6",
  "type": "module",
  "main": "index.ts",
  "files": [
    "assets/*"
  ],
  "scripts": {
    "clean": "rimraf assets coverage temp",
    "fix": "run-s prettier lint:fix",
    "prebuild": "npm run lint",
    "build": "tsc --project tsconfig.json --showConfig",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "29.2.5",
    "@typescript-eslint/eslint-plugin": "5.48.0",
